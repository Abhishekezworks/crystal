{
  "name": "grafast",
  "version": "0.0.0",
  "description": "Look-ahead engine for GraphQL",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js"
    },
    "./envelop": {
      "types": "./dist/envelop.d.ts",
      "node": "./dist/envelop.js"
    }
  },
  "scripts": {
    "test": "yarn test:mocha",
    "test:mocha": "mocha '**/__tests__/**/*-test.ts'",
    "prepack": "tsc -b",
    "md": "spec-md CRYSTAL_FLOW.md > CRYSTAL_FLOW.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/graphile-build.git"
  },
  "keywords": [
    "graphile",
    "graphql",
    "engine",
    "apollo",
    "graphqljs",
    "plugin",
    "build",
    "extension"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/graphile-build/issues"
  },
  "homepage": "https://graphile.org/graphile-build/",
  "dependencies": {
    "@graphile/lru": "4.6.0-alpha.0",
    "@types/node": "^16.11.15",
    "chalk": "^4.1.2",
    "eventemitter3": "^4.0.7",
    "iterall": "^1.3.0",
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  },
  "engines": {
    "node": ">=14.17"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@types/chai": "^4",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.178",
    "@types/mermaid": "^8",
    "@types/mocha": "^9.1.1",
    "@types/nodemon": "1.19.1",
    "chai": "^4.3.6",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "jest": "^28.1.0",
    "mermaid": "^8.14.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.15",
    "pg-sql2": "workspace:^",
    "spec-md": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.1-rc"
  }
}
