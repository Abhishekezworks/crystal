{
  "name": "graphile-exporter",
  "version": "0.0.1",
  "description": "Take a GraphQL schema from memory and write it to file",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "import": "./dist/helpers.js"
    }
  },
  "scripts": {
    "test": "jest -i",
    "prepack": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/graphile-build.git"
  },
  "keywords": [
    "graphql",
    "schema",
    "memory",
    "export",
    "output",
    "write",
    "file",
    "graphile"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/graphile-build/issues"
  },
  "homepage": "https://graphile.org/",
  "dependencies": {
    "@babel/generator": "^7.16.0",
    "@babel/parser": "^7.16.3",
    "@babel/template": "^7.16.0",
    "@types/node": "^16.11.6",
    "graphile-crystal": "4.7.0",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "files": ["dist"],
  "devDependencies": {
    "@types/babel__generator": "^7",
    "@types/babel__template": "^7",
    "@types/jest": "^27.0.2",
    "graphql": "npm:graphql@experimental-stream-defer",
    "jest": "^27.3.1",
    "ts-node": "^10.4.0",
    "typescript": "next"
  }
}
