{
  "name": "@dataplan/pg",
  "version": "4.7.0",
  "description": "PostgreSQL data plans for Graphile Crystal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "update-schema": "ts-node ./__tests__/exampleSchema.ts",
    "test:install-schema": "psql -X1v ON_ERROR_STOP=1 -f __tests__/schema.sql -f __tests__/sequence_reset.sql \"${TEST_DATABASE_URL:-graphile_crystal}\"",
    "test": "yarn test:install-schema && jest -i",
    "prepack": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/graphile-build.git"
  },
  "keywords": [
    "graphile",
    "graphql",
    "crystal",
    "postgresql",
    "postgres",
    "pg",
    "dataplan",
    "build",
    "extension"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/crystal/issues"
  },
  "homepage": "https://github.com/graphile/crystal/tree/main/packages/dataplan-pg",
  "dependencies": {
    "@graphile/lru": "4.6.0-alpha.0",
    "@types/node": "^16.11.6",
    "graphile-crystal": "4.7.0",
    "pg-sql2": "4.7.0",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": ">=14.17"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@dataplan/json": "4.7.0",
    "@types/jest": "^27.0.2",
    "@types/json5": "^2.2.0",
    "@types/nodemon": "1.19.1",
    "graphql": "npm:graphql@experimental-stream-defer",
    "jest": "^27.3.1",
    "jest-serializer-simple": "4.6.0",
    "json5": "^2.2.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
